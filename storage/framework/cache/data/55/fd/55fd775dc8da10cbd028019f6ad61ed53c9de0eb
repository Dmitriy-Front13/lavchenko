<script>
    document.addEventListener('DOMContentLoaded', function () {
        // Инициализация Fancybox
        Fancybox.bind("[data-fancybox]", {
            closeButton: false,
            dragToClose: false,
            closeExisting: true,
            on: {
                done: (fancybox) => {
                    document.querySelectorAll('.closeModal-btn').forEach(e => {
                        e.addEventListener('click', function () {
                            Fancybox.close(); 
                        });
                    });
                }
            }
        });

        // Проверка параметра showModal в URL
        var urlParams = new URLSearchParams(window.location.search);
        if (urlParams.get('showModal') === 'true') {
            // Открываем модальное окно с помощью Fancybox
            Fancybox.show([{ src: '#modal-mail', type: 'inline' }]);
        }
    });
</script>



