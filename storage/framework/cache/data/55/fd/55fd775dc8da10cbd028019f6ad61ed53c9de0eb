  {# <script>
const container = document.querySelector('.main-cells__box');

function adjustFontSize(cell) {
  const length = cell.textContent.length;
  const cellSize = parseFloat(getComputedStyle(container).getPropertyValue('--cell-size')); // Получаем текущий размер клетки
  let baseFontSize = cellSize * 0.4; // Базовый размер шрифта как 40% от размера клетки
  let fontSize;

  if (length < 3) {
      fontSize = baseFontSize; // Базовый размер шрифта
  } else if (length === 3) {
      fontSize = baseFontSize * 0.9; // Уменьшаем шрифт для трехзначных чисел
  } else {
      fontSize = baseFontSize * 0.8; // Ещё меньше для чисел с четырьмя и более цифрами
  }
  
  cell.style.fontSize = `${fontSize}px`; // Применяем размер шрифта в пикселях
}

function adjustSize() {
  const width = container.offsetWidth;
  const columns = 25;
  const gridGap = 8;
  const availableWidth = width - gridGap * (columns - 1);
  const calculatedSize = availableWidth / columns;
  container.style.setProperty('--cell-size', `${calculatedSize}px`);
  Array.from(container.children).forEach(adjustFontSize);
}



// Создание клеток
for (let i = 1; i <= 100; i++) {
  const cell = document.createElement('span');
  cell.classList.add('cell');
  cell.textContent = i;
  container.appendChild(cell);
  adjustFontSize(cell); // Применяем настройку шрифта
}

container.lastChild.textContent = 'K1';
adjustFontSize(container.lastChild); // Обновляем последнюю клетку

window.onload = adjustSize;
window.onresize = adjustSize;


